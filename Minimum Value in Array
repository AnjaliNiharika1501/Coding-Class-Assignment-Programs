public class Anjali {
  
  public static void main(String[] args) {
    int minValue= Integer.MAX_VALUE;
    int[] arr = {2, 6, 18, 9, 56, 44, 63, 22, 76, 28}; /

    for (int i = 0; i < arr.length; i++) {
      if(arr[i] < minValue) {
        minValue = arr[i];
      }
    }
    System.out.print("The minimum element in the array is= " + minValue); 
  }
}
