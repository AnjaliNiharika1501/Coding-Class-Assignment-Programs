public class Anjali {
  public static void main(String[] args) {
    int arr[] = {5434, 2476, -44654, 61321, 25548, 454564, 513444, 31231, 254};		 
   
    int max = arr[0];
    int min = arr[0];
    
    for(int i = 1; i < arr.length; i++){
      if(max < arr[i]){
        max = arr[i];
      } else if(min > arr[i]){
        min = arr[i];
      }		   		   
    }
    System.out.println("Maximum number = " 
         + max +  " Minimum number = " + min);		
  }
}
